#python calculator
operator = input("Enter the operator (+ * - /): ")
num1 = float(input("Enter value 1: "))
num2  = float(input("enter value 2: "))

if operator == "+":
    print(num1 + num2)
elif operator == "-":
    print(num1 - num2)
elif operator == '*':
    print(num1 * num2)
elif operator == "/":
    print(num1 / num2)
else:
    print(f"{operator} is invalid")
